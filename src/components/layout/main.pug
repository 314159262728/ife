doctype html
block variable
html
  head
    <!-- #{metadata.copyright} -->
    meta(charset="utf-8")
    meta(name="viewport",content="width=device-width,initial-scale=1")
    meta(http-equiv="X-UA-Compatible",content="IE=edge,chrome=1")
    meta(name="renderer",content="webkit")
    link(href=_image+"favicon.ico",rel="icon",type="image/x-icon")
    if _common&&_common.css
      link(href=_common.css,rel="stylesheet",type="text/css")
    if _assetFile&&_assetFile.css
      link(href=_assetFile.css,rel="stylesheet",type="text/css")

    title= (title||"Ding") +" | IFE"
  body
    block main
    if _common&&_common.js
      script(src=_common.js)
    if _assetFile&&_assetFile.js
      script(src=_assetFile.js)
